{
    "name": "jenky/laravel-api",
    "description": "Laravel API",
    "keywords": [
        "laravel",
        "api",
        "restapi"
    ],
    "homepage": "https://github.com/jenky/laravel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Linh Tran",
            "email": "jenky.w0w@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "~5.5",
        "illuminate/config": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelAPI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\LaravelAPI\\ApiServiceProvider"
            ]
        }
    },
    "suggest": {
       "barryvdh/laravel-cors": "Adds CORS (Cross-Origin Resource Sharing) headers support in your Laravel application (~0.9.3).",
       "spatie/laravel-fractal": "An easy to use Fractal wrapper built for Laravel applications (^5.0)."
    },
    "config": {
        "sort-packages": true
    }
}
