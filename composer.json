{
    "name": "jenky/laravel-api",
    "description": "Laravel API",
    "keywords": [
        "laravel",
        "api",
        "restapi"
    ],
    "homepage": "https://github.com/jenky/laravel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Linh Tran",
            "email": "contact@lynh.me"
        }
    ],
    "require": {
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/config": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelAPI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\LaravelAPI\\ApiServiceProvider"
            ]
        }
    },
    "suggest": {
       "barryvdh/laravel-cors": "Adds CORS (Cross-Origin Resource Sharing) headers support in your Laravel application.",
       "spatie/laravel-fractal": "An easy to use Fractal wrapper built for Laravel applications.",
       "spatie/laravel-query-builder": "Easily build Eloquent queries from API requests."
    },
    "config": {
        "sort-packages": true
    }
}
