{
    "name": "jenky/laravel-api",
    "description": "Laravel API",
    "keywords": [
        "laravel",
        "api",
        "restapi"
    ],
    "homepage": "https://github.com/jenky/laravel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Linh Tran",
            "email": "jenky.w0w@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "5.2.*|5.3.*|5.4.*",
        "illuminate/config": "5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.2.*|5.3.*|5.4.*",
        "barryvdh/laravel-cors": "^0.9.2",
        "spatie/laravel-fractal": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelAPI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\LaravelAPI\\ApiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
